#!/usr/bin/perl

use strict;
use Tratten::Monitored;

my %monitored = %{ &get_monitored };

print "{| border='1' cellspacing='0' class='wikitable sortable'\n! tratten link !! summary !! log file\n";

for (sort keys %monitored) {
  my ($url, $desc) = @{$monitored{$_}};
  print "|-\n| [[$_]] || $desc || [$url]\n";
}

print "|}\n";
